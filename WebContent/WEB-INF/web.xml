        <servlet-class>com.form.java.FormTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Form</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>edit</servlet-name>
        <servlet-class>com.form.java.EditTest</servlet-class>
    </servlet>
